        <style type="text/css">
        div.optionAdmin{
                padding: 40px 32px;
                display: grid;
                gap: 30px;
                grid-template-columns: repeat(3,auto);
        }
        div.userLogged{
                display: flex;
                align-items: flex-end;
                text-align: right;
        }
        div.container-fluid{
                background-color: #FFAEC7;
        }
        div.infoClient{
                text-align: center;
                margin-top: 10%;
        }
        body{
                background-color: #F1EBE5;
              
        }
        :root{
                font-family: "Barlow";
        }

        </style>
</head>
<body class="d-flex flex-column min-vh-100">
        <div class="mb-5">
                {{>_navbar}}
        </div>
                
        <div class="container mt-4 mb-5">
                {{#if error}}
                <div class="alert alert-danger">{{error}}</div>
                {{else}}
                {{/if}}
                {{>_msg}}
                {{{body}}}
        </div>
        <footer class="mt-auto">
                {{>_footer}}
        </footer>
</body>
